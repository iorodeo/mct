<launch>
    <!-- Autogenerated launch file - do not hand edit -->
    
    <include file="{{machine_file}}" />
    {% for camera_name, info in mjpeg_info_dict.iteritems() %}
    <node pkg="topic_tools" type="throttle" name="throttler_{{camera_name}}" args="messages {{info['image_topic']}} 10.0" machine="mct_master">
    </node>
    {% endfor %}

    {% for camera_name, info in mjpeg_info_dict.iteritems() %}
    <node pkg="mjpeg_server" type="mjpeg_server" name="{{info['mjpeg_server']}}" machine="mct_master">
        <param name="port" value="{{info['mjpeg_port']}}"/>
        <!-- <param name="image_transport" value="compressed" /> -->
    </node>
    {% endfor %}
</launch>
