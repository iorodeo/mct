<launch>
    <!-- Autogenerated launch file - do not hand edit -->
    <include file="{{machine_file}}" />
    {% for camera_name, camera_topic, image_topic, machine in calibrator_data %}
    <group ns="{{camera_topic}}">
        <node pkg="mct_camera_calibrator" type="cameracalibrator.py" name="camera_calibrator" args="--size {{chessboard_size}} --square {{chessboard_square}} image:={{image_topic}} camera:={{camera_topic}}" machine="{{machine}}"/>
    </group>
    {% endfor %} 
</launch>
