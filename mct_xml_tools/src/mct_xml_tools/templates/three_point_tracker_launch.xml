<launch>
    <!-- Autogenerated launch file - do not hand edit -->
    <include file="{{machine_file}}" />
    <group ns="three_point_tracker_params">
        <rosparam command="load" file="$(env MCT_CONFIG)/tracking_2d/three_point_tracker_params.yaml"/>
    </group>
    {% for region,launch_list in launch_dict.iteritems() %}
    <!-- {{region}} trackers -->
    {% for ns, topic, machine in launch_list %}
    <group ns="{{ns}}">
        <node pkg="mct_tracking_2d" type="three_point_tracker.py" name="three_point_tracker" args="{{topic}}" machine="{{machine}}"/>
    </group>
    {% endfor %}
    {% endfor %}
</launch>
