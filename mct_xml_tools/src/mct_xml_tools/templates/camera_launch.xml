<launch>
    <!-- Autogenerated launch file - do not hand edit -->
    <include file="{{machine_file}}" />
    {% for camera, info in camera_assignment.iteritems()%}
    <group ns="/{{info['computer']}}/{{camera}}" >
        {% if trigger %}
        <node pkg="camera1394_trig" type="camera1394_node" name="camera1394_node" machine="{{info['computer']}}" >
        {% else %}
        <node pkg="camera1394" type="camera1394_node" name="camera1394_node" machine="{{info['computer']}}" >
        {% endif %}
            <rosparam file="$(env MCT_CONFIG)/cameras/camera_common.yaml"/>
            <rosparam file="{{info['yaml_file']}}" />
            <param name="frame_rate" value="{{frame_rate}}" />
        </node>
    </group>
    {% endfor %}
</launch>
